{
  "name": "give-care-etl",
  "version": "0.3.0",
  "description": "Autonomous resource discovery and curation pipeline for GiveCare using OpenAI Agents SDK and Cloudflare Workers (3-agent architecture)",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "packageManager": "pnpm@9.0.0",
  "keywords": [
    "cloudflare-workers",
    "openai-agents",
    "resource-discovery",
    "web-scraping",
    "etl-pipeline"
  ],
  "author": "GiveCare",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.24",
    "@cloudflare/ai": "^1.0.0",
    "@cloudflare/puppeteer": "^1.0.4",
    "@openai/agents": "^0.1.11",
    "convex": "^1.29.0",
    "exa-js": "^1.10.2",
    "puppeteer-core": "^23.0.0",
    "zod": "^3.25.40"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "wrangler": "^3.90.0"
  }
}
